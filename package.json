{
    "name": "phonegap",
    "description": "PhoneGap command-line interface and node.js library.",
    "version": "3.5.0-0.20.00",
    "homepage": "http://github.com/phonegap/phonegap-cli",
    "repository": {
        "type": "git",
        "url": "git://github.com/phonegap/phonegap-cli.git"
    },
